<td>{{#if isDone}}<i class="icon-check">{{/if}}</i></td>
<td>{{name}}</td>
<td>{{humanTimeLength minutesEstimatedToComplete}}</td>
<td class="actions">
  {{#unless isDone}}
    <button class="btn btn-mini btn-primary" {{action markAsDone this}}>Completed</button>
  {{/unless}}
  <button class="btn btn-mini btn-warning" {{action enterEditMode this}}>Edit</button>
</td>
